{
  "name": "multicode-v3",
  "private": true,
  "workspaces": [
    "plugins/*"
  ],
  "scripts": {
    "setup": "node scripts/setup.js",
    "dev": "cargo run",
    "desktop:start": "cargo run -p desktop",
    "build": "cargo build",
    "test": "cargo test",
    "test:canvas": "node --test archived/frontend/src/visual/canvas/canvas.test.js",
    "clean": "cargo clean",
    "check:scripts": "node scripts/check-scripts.js",
    "check-env": "node scripts/check-env.js",
    "format": "prettier --write \"**/*.md\"",
    "format:check": "prettier --check \"**/*.md\""
  },
  "dependencies": {
    "fs-extra": "^11.3.1",
    "ora": "^5.4.1"
  },
  "devDependencies": {
    "prettier": "^3.2.5"
  }
}
