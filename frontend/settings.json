{
  "hotkeys": {
    "copyBlock": "Ctrl+C",
    "pasteBlock": "Ctrl+V",
    "selectConnections": "Ctrl+Shift+A",
    "focusSearch": "Ctrl+F",
    "showHelp": "Ctrl+?",
    "formatCurrentFile": "Shift+Alt+F"
  },
  "visual": {
    "theme": "default",
    "gridSize": 20,
    "showGrid": true,
    "syncOrder": true,
    "autoSync": false,
    "autoSyncInterval": 5
  },
  "editor": {
    "autoFoldMeta": true,
    "autoSync": false,
    "autoSyncInterval": 5,
    "spellcheck": false
  },
  "format": {
    "tabWidth": 2,
    "singleQuote": true,
    "semi": true
  },
  "plugins": {}
}
